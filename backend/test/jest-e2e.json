{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", {
      "tsconfig": "<rootDir>/tsconfig.json",
      "isolatedModules": true
    }]
  },
  "moduleNameMapper": {
    "^@gnosis/models$": "<rootDir>/../models/src/index.ts"
  }
} 